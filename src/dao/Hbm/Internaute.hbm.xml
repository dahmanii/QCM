<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

<class name="dao.Entites.Internaute" table="UTILISATEURS">

	<id name="id" column="ID">
		<generator class="native"></generator>
	</id>
	
	<property name="nom" column="NOM"></property>
	<property name="prenom" column="PRENOM"></property>
	<property name="identifiant" column="IDENTIFIANT"></property>
	<property name="password" column="PASSWORD"></property>
	<property name="role" column="ROLE"></property>
	<property name="dateNaissance" column="DATE_NAISSANCE"></property>
	<property name="points" column="POINTS"></property>
</class>

</hibernate-mapping>