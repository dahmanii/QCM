<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

<class name="dao.Entites.Choice" table="CHOICES">

	<id name="id" column="ID">
		<generator class="native"></generator>
	</id>	
	<property name="choix" column="CHOIX"></property>	
	<property name="label" column="LABEL"></property>	
	<property name="br" column="BR"></property>	
	
</class>

</hibernate-mapping>